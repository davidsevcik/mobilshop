<%= render :partial => 'admin/shared/content_manager_menu' %>

<%= error_messages_for :content_category %>
<h1>Editace kategorie</h1>

<% form_for(:content_category, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>

  <p class="form-buttons">
    <%= button t("update") %> nebo <%= link_to t('cancel'), collection_url %>
  </p>
<% end %>