<%= render :partial => 'admin/shared/tariff_manager_menu' %>

<h1>Editovat tarif</h1>
<%= error_messages_for :tariff %>

<% form_for(:tariff, :url => object_url, :html => { :method => :put }) do |f| -%>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("update") %> <%= t("or") %> <%= link_to t("cancel"), admin_tariffs_path %>
  </p>
<% end %>